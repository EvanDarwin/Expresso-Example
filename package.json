{
  "name": "expresso-demo",
  "version": "0.1.0",
  "description": "An example of an expresso application",
  "main": "build/index.js",
  "scripts": {
    "install": "tsc -b -v .",
    "start": "node build/index.js",
    "start:dev": "ts-node src/index.tsx",
    "watch": "nodemon -r ts-node/register ./src/index.tsx",
    "typeorm": "ts-node ./node_modules/expresso/node_modules/typeorm/cli.js",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "express",
    "framework",
    "example"
  ],
  "author": "Evan Darwin <evan@relta.net>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-formatter-gitlab": "^2.1.0",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^2.0.7",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "express": "^4.17.1",
    "expresso": "https://gitlab.com/Evan_Darwin/expresso.git#dev",
    "helmet": "^4.4.1",
    "jsx-xml": "^0.2.3",
    "pg": "^8.5.1",
    "preact": "^10.5.10",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.30"
  }
}
